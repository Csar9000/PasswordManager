<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="20dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rect_back_ground"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <View
                android:id="@+id/v10"
                android:layout_width="169dp"
                android:layout_height="166dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"
                android:background="@drawable/recr"
                android:foreground="@mipmap/credit_card2" />

            <LinearLayout
                android:id="@+id/linearLayout16"
                android:layout_width="222dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tView32"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Описание"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_desc_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tView41"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Банк"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_bank_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tv78"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Категория"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_item_cat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Данные карты"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearLayout17"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/recr"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tView40"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Номер карты"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_numberCard_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/recr"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tView43"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Дата истечения срока"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_item_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/recr"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout12"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/recr"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tView42"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Пароль"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_cardbank_item_password"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/recr"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/tView44"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="CVV/CVC"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_bankcard_item_cvv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/recr"
                    android:textAlignment="center"
                    android:textColor="#050505"
                    android:textSize="18sp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>